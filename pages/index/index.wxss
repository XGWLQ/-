.swiper_index swiper {
  width: 750rpx;
  height: 340rpx;
}
.nav_index {
  display: flex;
}
.nav_index navigator {
  flex: 1;
  padding: 20rpx;
}
.floor_index .floor_item .floor_title {
  margin: 10rpx 0;
}
.floor_index .floor_item .floor_list {
  overflow: hidden;
}
.floor_index .floor_item .floor_list navigator {
  float: left;
  width: 33.33%;
  /* 2 3 两个超链接 */
}
.floor_index .floor_item .floor_list navigator:nth-child(2),
.floor_index .floor_item .floor_list navigator:nth-child(3),
.floor_index .floor_item .floor_list navigator:nth-child(4),
.floor_index .floor_item .floor_list navigator:nth-child(5) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.floor_index .floor_item .floor_list navigator:nth-child(2),
.floor_index .floor_item .floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.floor_index .floor_item .floor_list navigator image {
  width: 100%;
  height: 100%;
}
