<tabs tabs="{{tabs}}" bind:itemChange="handleTabChenge">
	<view class="order_content"
	wx:for="{{ordersList}}"
	wx:key="order_id">
		<view class="order_no order_no_row">
			<view class="no_text">订单编号</view>
			<view class="no_id">{{item.order_number}}</view>
		</view>
		<view class="order_no order_price_row">
			<view class="no_text">订单价格</view>
			<view class="no_id">￥{{item.order_price}}</view>
		</view>
		<view class="order_no order_time_row">
			<view class="no_text">订单日期</view>
			<view class="no_id">{{item.create_time_cn}}</view>
		</view>
	</view>
</tabs>
