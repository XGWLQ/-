<view class="search_warp">
	<input type="text" placeholder="请输入搜索的内容" value="{{inpValue}}" bindinput="HandleInput"/>
	<button hidden="{{!isFocus}}" bind:tap="handleCancel">取消</button>
</view>
<view class="sarch_content">
	<navigator
	url="/pages/goods_detail/index?goods_id={{item.goods_id}}"
	bind:tap="handleGoodItem" 
	class="search_item"
	wx:for="{{searchList}}"
	wx:key="goods_id"
	>
		{{item.goods_name}}
	</navigator>
</view>
